      <property name="styleSheet">
       <string notr="true">/* Basic QSlider styling */
QSlider {
    background: transparent;
}

/* Styling for the groove (the background of the slider) */
QSlider::groove:horizontal {
    border: 1px solid #999999;
    height: 8px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #c4c4c4);
    margin: 2px 0;
    border-radius: 4px;
}

QSlider::groove:vertical {
    border: 1px solid #999999;
    width: 8px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #B1B1B1, stop:1 #c4c4c4);
    margin: 0 2px;
    border-radius: 4px;
}

/* Styling for the handle (the draggable part) */
QSlider::handle:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
    border: 1px solid #5c5c5c;
    width: 18px;
    margin: -4px 0; /* expand outside the groove */
    border-radius: 9px;
}

QSlider::handle:vertical {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
    border: 1px solid #5c5c5c;
    height: 18px;
    margin: 0 -4px; /* expand outside the groove */
    border-radius: 9px;
}

/* Styling for the add-page (the part before the handle) */
QSlider::add-page:horizontal {
    background: #5c5c5c;
    border: 1px solid #999999;
    height: 8px;
    border-radius: 4px;
}

QSlider::add-page:vertical {
    background: #5c5c5c;
    border: 1px solid #999999;
    width: 8px;
    border-radius: 4px;
}

/* Styling for the sub-page (the part after the handle) */
QSlider::sub-page:horizontal {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #6a9ae2, stop:1 #1a5dc9);
    border: 1px solid #0a4a9b;
    height: 8px;
    border-radius: 4px;
}

QSlider::sub-page:vertical {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #6a9ae2, stop:1 #1a5dc9);
    border: 1px solid #0a4a9b;
    width: 8px;
    border-radius: 4px;
}

/* Optional: Style for when the slider is pressed */
QSlider::handle:horizontal:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #dadada, stop:1 #8f8f8f);
}

QSlider::handle:vertical:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #dadada, stop:1 #8f8f8f);
}</string>
